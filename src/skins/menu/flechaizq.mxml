<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009"
				   xmlns:s="library://ns.adobe.com/flex/spark"
				   xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
				   xmlns:botones="fxg.menu.*"
				   width="110" height="95" minWidth="21" minHeight="21"
				   alpha.disabled="0.5">
	
	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>
	
	<!-- states -->
	<s:states>
		<s:State name="up" />
		<s:State name="over" />
		<s:State name="down" />
		<s:State name="disabled" />
	</s:states>
	
	<botones:flechaizqup includeIn="up, disabled" width="100%" height="100%"/>
	<botones:flechaizqover includeIn="over, down" width="100%" height="100%"/>
	
</s:SparkButtonSkin>